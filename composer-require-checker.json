{
    "php-core-extensions": [
        "Core",
        "ctype",
        "date",
        "filter",
        "hash",
        "mbstring",
        "pcre",
        "Reflection",
        "SPL",
        "standard"
    ],
    "scan-files": [
        "Foo.php"
    ],
    "symbol-whitelist": [
        "LARAVEL_START",

        "dump",
        "make",

        "GuzzleHttp\\MessageFormatter",
        "GuzzleHttp\\Middleware",
        "PhpParser\\Node",
        "PhpParser\\Node\\Expr\\New_",
        "PhpParser\\Node\\Name",
        "Psr\\Http\\Message\\ResponseInterface",
        "Rector\\Contract\\Rector\\ConfigurableRectorInterface",
        "Rector\\Rector\\AbstractRector",
        "Symfony\\Component\\Console\\Input\\InputInterface",
        "Symfony\\Component\\Console\\Output\\OutputInterface",
        "Symfony\\Component\\ErrorHandler\\Exception\\FlattenException",
        "Symplify\\RuleDocGenerator\\ValueObject\\CodeSample\\ConfiguredCodeSample",
        "Symplify\\RuleDocGenerator\\ValueObject\\RuleDefinition",
        "Webmozart\\Assert\\Assert"
    ]
}